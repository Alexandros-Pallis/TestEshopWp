{
   "name": "wordpress/testeshop",
   "authors": [
      {
         "name": "Alexandros-Pallis",
         "email": "alexanpall@gmail.com"
      }
   ],
   "require-dev": {
      "wpackagist-plugin/woocommerce": "^6.5",
      "wpackagist-plugin/autoconvert-greeklish-permalinks": "^4.0",
      "metorikhq/wc-cyclone": "master"
   },
   "repositories": [
      {
         "type": "composer",
         "url": "https://wpackagist.org"
      },
      {
         "type": "vcs",
         "url": "https://github.com/metorikhq/wc-cyclone.git"
      }
   ],
   "extra": {
      "installer-paths": {
         "plugins/{$name}": [
            "type:wordpress-plugin"
         ]
      }
   },
   "config": {
      "allow-plugins": {
         "composer/installers": true
      }
   }
}
